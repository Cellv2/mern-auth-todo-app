{
    "name": "mern-auth-todo-app",
    "version": "0.0.1",
    "description": "A MERN stack todo app with authentication",
    "author": "Cellv2",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Cellv2/mern-auth-todo-app.git"
    },
    "bugs": {
        "url": "https://github.com/Cellv2/mern-auth-todo-app/issues"
    },
    "homepage": "https://github.com/Cellv2/mern-auth-todo-app#readme",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "concurrently -k -p \"[{name}]\" -n \"client,ts,server\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run start-client\" \"npm run watch-ts\" \"npm run watch-server\"",
        "test": "jest",
        "build": "echo \"This command has not yet been set up - Exiting\" && exit 1",
        "build-server": "tsc",
        "postinstall": "npm run install-client",
        "install-client": "cd client && npm i && cd ..",
        "install-server": "npm install",
        "start-client": "cd client && npm start",
        "start-server": "node --experimental-specifier-resolution=node dist/server/server.js",
        "watch-ts": "tsc -w",
        "watch-server": "nodemon --experimental-specifier-resolution=node dist/server/server.js"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.5.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^9.0.0",
        "mongodb": "^3.5.7",
        "mongoose": "^5.9.11",
        "nodemon": "^2.0.3",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "react-bootstrap": "^1.0.1",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.1.2",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.5.0",
        "@testing-library/react": "^10.0.3",
        "@testing-library/user-event": "^10.1.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.6",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.6",
        "@types/jest": "^25.2.1",
        "@types/jsonwebtoken": "^8.3.9",
        "@types/mongodb": "^3.5.14",
        "@types/mongoose": "^5.7.14",
        "@types/node": "^13.13.2",
        "@types/passport": "^1.0.3",
        "@types/passport-jwt": "^3.0.3",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/supertest": "^2.0.8",
        "@types/validator": "^13.0.0",
        "concurrently": "^5.2.0",
        "jest": "^29.3.1",
        "redux-devtools-extension": "^2.13.8",
        "supertest": "^4.0.2",
        "ts-jest": "^25.4.0",
        "typescript": "^3.8.3"
    },
    "engines": {
        "node": "14.0.0"
    }
}
